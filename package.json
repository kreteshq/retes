{
  "name": "retes",
  "version": "0.24.0",
  "description": "Universal HTTP library for Node.js, Deno & Serverless (Cloudflare, Vercel, Netlify)",
  "types": "index.d.ts",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pnpm build && clear-package-json package.json -o dist/package.json --fields publishConfig",
    "watch": "tsc -w",
    "build": "tsup-node src/* --format esm,cjs --dts",
    "test": "uvu -r tsm spec",
    "test-watch": "watchlist src spec -- pnpm test"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/node": "^17.0.35",
    "axios": "^0.27.2",
    "clean-publish": "^4.0.0",
    "form-data": "^4.0.0",
    "next": "^12.1.6",
    "tsm": "^2.2.1",
    "tsup": "^6.0.1",
    "typescript": "4.7.2",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1"
  },
  "exports": {
    "./package.json": "./package.json",
    "./middleware": {
      "types": "./middleware.d.ts",
      "import": "./middleware.mjs",
      "require": "./middleware.js"
    },
    "./response": {
      "types": "./response.d.ts",
      "import": "./response.mjs",
      "require": "./response.js"
    },
    "./route": {
      "types": "./route.d.ts",
      "import": "./route.mjs",
      "require": "./route.js"
    },
    "./adapter": {
      "types": "./adapter.d.ts",
      "import": "./adapter.mjs",
      "require": "./adapter.js"
    },
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "publishConfig": {
    "directory": "dist"
  }
}
