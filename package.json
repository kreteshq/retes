{
  "name": "retes",
  "version": "0.23.0--DEV",
  "description": "Universal HTTP library for Node.js, Deno & Serverless (Cloudflare, Vercel, Netlify)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm build && cp package.json dist/",
    "watch": "tsc -w",
    "build": "tsup-node src/* --format esm,cjs --dts",
    "test": "uvu -r tsm spec",
    "test-watch": "watchlist  src spec -- pnpm test"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.34",
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "tsm": "^2.2.1",
    "tsup": "^5.12.8",
    "typescript": "4.7.1-rc",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1"
  },
  "exports": {
    "./package.json": "./package.json",
    "./response": {
      "types": "./dist/response.d.ts",
      "import": "./dist/response.mjs",
      "require": "./dist/response.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "directory": "dist"
  }
}
